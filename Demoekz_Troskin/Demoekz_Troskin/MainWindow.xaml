<Window x:Class="Demoekz_Troskin.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Demoekz_Troskin"
        mc:Ignorable="d"
        Title="Avtodiler" Height="450" Width="800"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="200*"/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Background="#FFFA" Orientation="Horizontal">
        </StackPanel>
        <Button x:Name="btnBack" Content="Back" HorizontalAlignment="Right" VerticalAlignment="Center" Click="btnBack_Click" Margin="5" Style="{StaticResource menuBtn}"/>
        <Frame x:Name="FrmMain" Grid.Row="1" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
